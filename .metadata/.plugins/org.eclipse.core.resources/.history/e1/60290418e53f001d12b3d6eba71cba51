#include "AIC3254.h"

/*
 * INIZIALIZE
 */



/*
 * LOW-LEVEL FUNCTIONS
 */

HAL_StatusTypeDef AIC3254_SendCommand(AIC3254 *dev, uint8_t Register, uint8_t Data) {

	return HAL_I2C_Mem_Write(dev->i2cHandle, AIC3254_DEV_ADDR , Register, I2C_MEMADD_SIZE_8BIT, Data, 1, HAL_MAX_DELAY);
}
