/*
 * LOW-LEVEL FUNCTIONS
 */

HAL_StatusTypeDef AIC3254_SendCommand(AIC3254 *dev, uint8_t address, uint8_t *data) {

	return HAL_I2C_Mem_Write(dev->i2cHandle, AIC3254_DEV_ADDR , MemAddress, MemAddSize, pData, Size, Timeout)
}
